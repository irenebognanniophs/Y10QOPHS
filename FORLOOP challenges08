direction = str(input("in which direction do you want to count? u/d "))
if direction == "u":
  topnum= int(input("Enter the top number >>> "))
  for i in range (1, topnum+1, +1 ):
   print(i)
  
elif direction == "d":
  num= int(input("Enter a number below 20 >>> "))
  for i in range (20, num-1, -1):
   print(i)
  
else:
  print("I don't understand")
